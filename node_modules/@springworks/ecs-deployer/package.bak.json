{
  "name": "@springworks/ecs-deployer",
  "version": "2.10.0",
  "description": "updates an ECS service with a new TaskDefinition",
  "main": "ecs-deployer.js",
  "scripts": {
    "test": "istanbul cover _mocha",
    "lint": "eslint --cache .",
    "test-no-cov": "NODE_ENV=test mocha",
    "test-acceptance": "NODE_ENV=test istanbul cover _mocha -- --fgrep '/acceptance/'",
    "test-component": "NODE_ENV=test istanbul cover _mocha -- --fgrep '/component/'",
    "test-unit": "NODE_ENV=test istanbul cover _mocha -- --fgrep '/unit/'",
    "build": "rm -rf build/* && babel src --out-dir build && cp package.json build/ && cp README.md build/"
  },
  "bin": {
    "ecs-deployer": "ecs-deployer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Springworks/node-ecs-deployer.git"
  },
  "author": "Springworks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Springworks/node-ecs-deployer/issues"
  },
  "homepage": "https://github.com/Springworks/node-ecs-deployer#readme",
  "devDependencies": {
    "@springworks/test-harness": "^1.5.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-register": "^6.18.0",
    "eslint": "^3.13.1",
    "eslint-config-springworks": "^7.0.7",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-should-promised": "^2.0.0",
    "eslint-plugin-springworks": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "@springworks/input-validator": "^5.0.1",
    "aws-sdk": "^2.7.22",
    "lodash": "^4.17.5"
  }
}
